{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"strict": true,
		"incremental": true,
		"outDir": "dist",
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": false,
		"noEmit": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
